{
  "name": "naep-covid-app",
  "version": "0.0.1",
  "description": "The COVID Incident Management System",
  "productName": "The COVID Incident Management System",
  "private": true,
  "scripts": {
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "live:dev": "STAGE=dev quasar dev",
    "live:test": "STAGE=test quasar dev",
    "live:train": "STAGE=train quasar dev",
    "live:prod": "STAGE=prod quasar dev",
    "winlive:dev": "SET STAGE=dev&&quasar dev",
    "winlive:test": "SET STAGE=test&&quasar dev",
    "winlive:train": "SET STAGE=train&&quasar dev",
    "winlive:prod": "SET STAGE=prod&&quasar dev",
    "startclean": "rimraf ./node_modules/.cache/@babel && nodemon --exec \"node server/index.mjs\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "dummy": "--- items below are for CI/CD builds",
    "build:ci:dev": "STAGE=dev quasar build ",
    "build:ci:test": "STAGE=test quasar build ",
    "build:ci:train": "STAGE=train quasar build ",
    "build:ci:prod": "STAGE=prod quasar build ",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.44.0",
    "@quasar/extras": "^1.11.0",
    "@vue/apollo-util": "^4.0.0-alpha.16",
    "apollo-boost": "^0.4.9",
    "aws-appsync": "^4.0.1",
    "axios": "^0.22.0",
    "crypto-js": "^4.1.1",
    "emailjs-com": "^3.2.0",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "loading": "^1.13.3",
    "luxon": "^2.3.0",
    "quasar": "^2.0.0",
    "sass": "^1.49.0",
    "showdown": "^1.9.1",
    "vue-apollo": "^3.0.5",
    "vue-async-computed": "^3.9.0",
    "vue-i18n": "^9.2.0-beta.23",
    "vuex": "4",
    "vuex-composition-helpers": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@quasar/app": "^3.0.0",
    "@quasar/quasar-app-extension-apollo": "^2.0.0-beta.3",
    "@quasar/quasar-app-extension-qmarkdown": "^2.0.0-alpha.9",
    "@quasar/quasar-app-extension-testing": "^2.0.1",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.7",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue3": "^6.4.9",
    "apollo-server": "^3.5.0",
    "babel-loader": "^8.2.3",
    "css-loader": "5.2.0",
    "eslint-plugin-jest": "^25.2.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "vue-loader": "^16.8.3",
    "workbox-webpack-plugin": "^6.0.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}